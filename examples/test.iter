func gcd a b
	iter b
		a = % a b
		a = ^ a b
		b = ^ a b
		a = ^ a b
	stop
	func = a
stop


func iota A n ini
	E = + A n
	iter != A E
		A <- ini
		A = ++ A
		ini = ++ ini
	stop
stop


func accumulate A n
	sum = null
	E = + A n
	iter != A E
		sum = + sum [] A
		A = ++ A
	stop
	func = sum
stop


func max_element A n
	max = -0x8000000000000000
	E = + A n
	iter != A E
		once < max [] A
			max = [] A
		stop
	stop
	func = max
stop


func AFill n x
	A = .Heap n
	i = null
	iter < i n
		I = + A i
		I <- x
		i = ++ i
	stop
	func = A
stop


type ipair
	x y
stop


func ipair_New x y
	ipair_R = .Heap 2
	X = ipair@x ipair_R
	Y = ipair@y ipair_R
	X <- x
	Y <- y
	func = ipair_R
stop


func sumIpair ipair_P
	func = + ipair:x ipair_P ipair:y ipair_P
stop


func binPow a n m
	once ! n
		func = 1
		halt true
	stop
	b = .binPow a >> n 1 m
	b = % * b b m
	once & 1 n
		b = % * b a m
	stop
	func = b
stop


func Erat n
	A = .Afill ++ n 1
	A <- null
	I = ++ A
	I <- null
	i = 2
	iter <= * i i n
		I = + A i
		once ! [] I
		pass true
			j = * i i
			iter <= j n
				I = + A j
				I <- null
				j = + j i
			stop
		stop
		i = ++ i
	stop
	func = A
stop


func main

stop
